{
  "settings": {
    "analysis": {
      "analyzer": {
        "evprop_ngram_analyzer": {
          "tokenizer": "evprop_ngram_tokenizer"
        },
        "evprop-autocomplete": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "standard",
            "lowercase",
            "ngram"
          ]
        }
      },
      "tokenizer": {
        "evprop_ngram_tokenizer": {
          "type": "nGram",
          "min_gram": "3",
          "max_gram": "12",
          "token_chars": [
            "letter",
            "digit"
          ]
        }
      },
      "filter": {
        "ngram": {
          "type": "ngram",
          "min_gram": 2,
          "max_gram": 18
        }
      }
    }
  },
  "mappings": {
    "collection": {
      "_source": {
        "enabled": true
      },
      "properties": {
        "key": {
          "type": "string",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "name": {
          "type": "string",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "colPath": {
          "type": "string",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "href": {
          "type": "string",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "deleted": {
          "type": "boolean",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "creatorHref": {
          "type": "string",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "ownerHref": {
          "type": "string",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "acl": {
          "type": "string",
          "include_in_all": false,
          "index": "no"
        },
        "public": {
          "type": "boolean",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "tombstoned": {
          "type": "boolean",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "created": {
          "type": "date",
          "format": "basic_date_time_no_millis",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "lastModified": {
          "type": "date",
          "format": "basic_date_time_no_millis",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "lastModifiedSeq": {
          "type": "integer",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "summary": {
          "type": "string",
          "include_in_all": true
        },
        "description": {
          "type": "string",
          "include_in_all": true
        },
        "affectsFreeBusy": {
          "type": "boolean",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "aliasURI": {
          "type": "string",
          "include_in_all": true
        },
        "caltype": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "display": {
          "type": "boolean",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "filterExpr": {
          "type": "string",
          "include_in_all": true
        },
        "ignoreTransp": {
          "type": "boolean",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "lastRefresh": {
          "type": "string",
          "include_in_all": true
        },
        "lastRefreshStatus": {
          "type": "string",
          "include_in_all": true
        },
        "refreshRate": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "remoteId": {
          "type": "string",
          "include_in_all": true
        },
        "remotePw": {
          "type": "string",
          "include_in_all": true
        },
        "unremoveable": {
          "type": "boolean",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "colProperties": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "include_in_all": false,
              "index": "not_analyzed"
            },
            "value": {
              "type": "string",
              "include_in_all": false,
              "index": "not_analyzed"
            }
          }
        },
        "categories": {
          "type": "object",
          "properties": {
            "uid": {
              "type": "string",
              "include_in_all": false,
              "index": "not_analyzed"
            },
            "href": {
              "type": "string",
              "include_in_all": false,
              "index": "not_analyzed"
            },
            "value": {
              "type": "object",
              "properties": {
                "language": {
                  "type": "string",
                  "include_in_all": false
                },
                "value": {
                  "type": "string",
                  "include_in_all": true
                }
              }
            }
          }
        }
      }
    }
  }
}
