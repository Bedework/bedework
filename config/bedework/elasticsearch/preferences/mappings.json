        },
        "scheduleAutoProcessResponses": {
          "type": "integer",
          "include_in_all": false,
          "index": "not_analyzed"
        },
        "userProperties": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "include_in_all": false,
              "index": "not_analyzed"
